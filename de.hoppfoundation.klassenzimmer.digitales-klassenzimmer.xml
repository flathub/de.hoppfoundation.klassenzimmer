<?xml version="1.0"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="x-scheme-handler/jitsi-meet">
    <icon name="org.jitsi.jitsi-meet"/>
    <generic-icon name="applications-internet"/>
    <comment>Join Jitsi meeting</comment>
    <glob weight="5" pattern="jitsi-meet://*"/>
  </mime-type>
</mime-info>
